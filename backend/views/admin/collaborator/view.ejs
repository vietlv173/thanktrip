                                                        </thead>
                                                        <tbody>
                                                        <tr>
                                                            <td></td>
                                                            <td></td>
                                                            <td></td>
                                                            <td></td>
                                                            <td class="th-text-right">0</td>
                                                        </tr>
                                                        <tr ng-repeat="(key, transaction) in responses.transactions">
                                                            <td>{{ key+1 }}</td>
                                                            <td>
                                                                <div>
                                                                    {{ transaction.bank.name }} - {{ transaction.bank.branch }}
                                                                </div>
                                                                <div class="small text-muted" style="font-size: 90%;">

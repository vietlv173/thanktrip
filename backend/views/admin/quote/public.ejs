    <!-- Main styles for this application-->
    <link href="/stylesheets/style.css" rel="stylesheet">
    <link href="/stylesheets/pace.min.css" rel="stylesheet">

    <style>
        .card-body table th {
            text-transform: uppercase;
        }

        table tr th {
            color: #000 !important;
            border-top: 1px solid #c5e7f2 !important;
            border-bottom: 1px solid #c5e7f2 !important;
            background: url(https://cdn-app.kiotviet.vn/retailler/Content/thBg.png) right center no-repeat #dcf4fc !important;
        }

        .table th {
            border-bottom: none !important;
            font-weight: 400 !important;
            font-size: 12px !important;
            text-transform: uppercase;
        }

        .table-responsive-sm a:hover {
            text-decoration: none;
        }

    </style>

    <style>
        h2 {
            line-height: 36px;
            font-size: 20px;
        }

        h5 {
            line-height: 18px;
            font-size: 16px;
            font-weight: 400;
        }

        .r4_counter h4 {
            margin: 0;
        }

        a {
            color: #008ece;
            text-decoration: none !important;
        }

        .dashboardStatistic {
            position: relative;
        }

        .uln ul {
            list-style: none;
            margin: 0;
            padding: 0;
        }

        .dashboard-custom .dashboardStatistic ul li {
            max-width: calc(100% / 4);
        }

        .dashboardStatistic ul li {
            position: relative;
            color: #999;
            font-size: 12px;
            padding: 6px 0 6px 67px;
            text-align: left;
            display: inline-block;
            height: auto;
            vertical-align: middle;
            min-height: 72px;
            white-space: nowrap;
            margin: 4px 25px 8px 0;
            box-sizing: border-box;
        }

        .dashboardStatistic .total .dash_icon {
            background: #0094da;
        }

        .dashboardStatistic .dash_icon {
            display: inline-block;
            width: 30px;
            height: 30px;
            margin: 0;
            position: absolute;
            top: 25px;
            left: 28px;
            background: #4bac4d;
            text-align: center;
            border-radius: 100% !important;
            color: #fff;
            line-height: 32px;
        }

        .dashboardStatistic .dash_title {
            font-weight: 700;
            color: #666;
            margin: 0 0 0 2px;
        }

        .c-callout {
            text-align: left;
            padding: 6px 0 6px 60px;
            border-top: 1px solid #d6d6d6;
        }

        .dashboardStatistic .total span.number {
            color: #0094da;
        }

        .dashboardStatistic span.number {
            display: block;
            font-size: 18px;
            line-height: 28px;
        }

        span.yesterday {
            color: #999;
            display: block;
            font-size: 12px;
            margin: 0 0 0 2px;
        }

        .btn-group-left a, .btn-group-left button {
            text-transform: uppercase;
        }

        .dashboardStatistic .return .dash_icon {
            background: #ff8030;
        }

        .dashboardStatistic .down .dash_icon {
            background: #f94f5e;
        }

        @media (min-width: 768px) {
            .c-callout {
                border-top: none;
                border-left: 1px solid #d6d6d6;
            }
        }
    </style>

    <style type="text/css">
        @keyframes spinner {
            0% {
                opacity: 1
            }
            100% {
                opacity: 0
            }
        }

        .spinner div {
            left: 95px;
            top: 75px;
            position: absolute;
            animation: spinner linear 1s infinite;
            background: #1d3f72;
            width: 10px;
            height: 10px;
            border-radius: 5px / 5px;
            transform-origin: 5px 25px;
        }

        .spinner div:nth-child(1) {
            transform: rotate(0deg);
            animation-delay: -0.9s;
            background: #1d3f72;
        }

        .spinner div:nth-child(2) {
            transform: rotate(36deg);
            animation-delay: -0.8s;
            background: #1d3f72;
        }

        .spinner div:nth-child(3) {
            transform: rotate(72deg);
            animation-delay: -0.7s;
            background: #1d3f72;
        }

        .spinner div:nth-child(4) {
            transform: rotate(108deg);
            animation-delay: -0.6s;
            background: #1d3f72;
        }

        .spinner div:nth-child(5) {
            transform: rotate(144deg);
            animation-delay: -0.5s;
            background: #1d3f72;
        }

        .spinner div:nth-child(6) {
            transform: rotate(180deg);
            animation-delay: -0.4s;
            background: #1d3f72;
        }

        .spinner div:nth-child(7) {
            transform: rotate(216deg);
            animation-delay: -0.3s;
            background: #1d3f72;
        }

        .spinner div:nth-child(8) {
            transform: rotate(252deg);
            animation-delay: -0.2s;
            background: #1d3f72;
        }

        .spinner div:nth-child(9) {
            transform: rotate(288deg);
            animation-delay: -0.1s;
            background: #1d3f72;
        }

        .spinner div:nth-child(10) {
            transform: rotate(324deg);
            animation-delay: 0s;
            background: #1d3f72;
        }

        .loading-io {
            width: 200px;
            height: 200px;
            display: inline-block;
            overflow: hidden;
        }

        .spinner {
            width: 100%;
            height: 100%;
            position: relative;
            transform: translateZ(0) scale(1);
            backface-visibility: hidden;
            transform-origin: 0 0; /* see note above */
        }

        .spinner div {
            box-sizing: content-box;
        }

        .th-text-right {
            text-align: right;
        }

        .tab-content .tab-pane {
            padding: 0 0 0;
        }
    </style>
</head>

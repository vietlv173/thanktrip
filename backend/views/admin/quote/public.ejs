                                <div class="card-body" style="padding: 0;min-height: calc(100vh - 131px);">
                                    <div style="padding: 1.25rem;">
                                        <strong style="text-transform: uppercase;">Báo giá khách sạn</strong>
                                        <br>
                                        <span>Ngày tạo: <%= quote.createdAt %></span>
                                    </div>
                                    <div>
                                        <div style="background: #F2F2F2;padding: 10px 20px;font-weight: bold;text-transform: uppercase;">
                                            Option <%= key1 + 1 %>: <%= detail.hotel.title %>
                                        </div>
                                        <div style="padding: 1.25rem;">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="col-md-12 mb-3">
                                                            <strong>Danh sách phòng</strong>
                                                        </div>
                                                    </div>
                                                    <div style="background: #f2f2f2;border-radius: 10px;padding: 10px 15px 0;">
                                                        <% detail.hotel.room_detail.forEach(function(room, key) { %>
                                                            <% if(detail.rooms.includes(room.id)) { %>
                                                                <div class="row">
                                                                    <div class="col-md-6">
                                                                        <div class="form-group">
                                                                            <% if(key === 0) { %>
                                                                                <label for="">
                                                                                    Loại phòng
                                                                                </label>
                                                                            <% } %>
                                                                            <p style="margin-top: 7px;">
                                                                                <strong>
                                                                                    <%= room.room_type + ', ' + (room.bed_type === 1 ? 'TWIN (TWN)' : 'Double (DBL)') %>
                                                                                </strong>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-2"
                                                                         style="text-align: right;">
                                                                        <div class="form-group">
                                                                            <% if(key === 0) { %>
                                                                                <label for="">
                                                                                    Ngày thường
                                                                                </label>
                                                                            <% } %>
                                                                            <p style="margin-top: 7px;">
                                                                                ₫<strong>
                                                                                    <%= _.formatNumber(room.price_normal + detail.hotel.surcharge) %>
                                                                                </strong>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-2"
                                                                         style="text-align: right;">
                                                                        <div class="form-group">
                                                                            <% if(key === 0) { %>
                                                                                <label for="">
                                                                                    Cuối tuần
                                                                                </label>
                                                                            <% } %>
                                                                            <p style="margin-top: 7px;">
                                                                                ₫<strong>
                                                                                    <%= _.formatNumber(room.price_weekend + detail.hotel.surcharge) %>
                                                                                </strong>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-2"
                                                                         style="text-align: right;">
                                                                        <div class="form-group">
                                                                            <% if(key === 0) { %>
                                                                                <label for="">
                                                                                    Ngày lễ
                                                                                </label>
                                                                            <% } %>
                                                                            <p style="margin-top: 7px;">
                                                                                ₫<strong>
                                                                                    <%= _.formatNumber(room.price_lunar + detail.hotel.surcharge) %>
                                                                                </strong>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            <% } %>
                                                        <% }); %>
                                                    </div>
                                                    <% if(detail.hotel.service_detail.length) { %>
                                                        <div class="row">
                                                            <div class="col-md-12 mb-3 mt-3">
                                                                <strong>Phụ thu</strong>
                                                            </div>
                                                        </div>
                                                        <div style="background: #f2f2f2;border-radius: 10px;padding: 10px 15px 0;">
                                                            <% detail.hotel.service_detail.forEach(function(service, key) { %>
                                                                <div class="row">
                                                                    <div class="col-md-6">
                                                                        <div class="form-group">
                                                                            <% if(key === 0) { %>
                                                                                <label for="">
                                                                                    Tên dịch vụ
                                                                                </label>
                                                                            <% } %>
                                                                            <p style="margin-top: 7px;">
                                                                                <strong>
                                                                                    <%= service.title %>
                                                                                </strong>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-2"></div>
                                                                    <div class="col-md-2"></div>
                                                                    <div class="col-md-2" style="text-align: right;">
                                                                        <div class="form-group">
                                                                            <% if(key === 0) { %>
                                                                                <label for="">
                                                                                    Chi phí
                                                                                </label>
                                                                            <% } %>
                                                                            <p style="margin-top: 7px;">
                                                                                ₫<strong>
                                                                                    <%= _.formatNumber(service.price) %>
                                                                                </strong>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            <% }); %>
                                                        </div>
                                                    <% } %>
                                                    <div class="row">
                                                        <div class="col-md-12 mb-3 mt-3">
                                                            <strong>Thông tin khách sạn</strong>
                                                        </div>
                                                    </div>
                                                    <div style="background: #f2f2f2;border-radius: 10px;padding: 10px 15px 0;">
                                                        <div class="row">
                                                            <div class="col-md-4">
                                                                <div class="form-group">
                                                                    <label for="">Giờ nhận phòng</label>
                                                                    <p style="margin-top: 7px;">
                                                                        <strong>
                                                                            <%= detail.hotel.check_in %>
                                                                        </strong>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-4" style="text-align: center;">
                                                                <div class="form-group">
                                                                    <label for="">Giờ trả phòng</label>
                                                                    <p style="margin-top: 7px;">
                                                                        <strong>
                                                                            <%= detail.hotel.check_out %>
                                                                        </strong>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-4" style="text-align: right;">
                                                                <div class="form-group">
                                                                    <label for="">Chính sách hoàn huỷ</label>
                                                                    <p style="margin-top: 7px;">
                                                                        <strong>
                                                                            <%= detail.hotel.refund_policy %>
                                                                        </strong>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div style="background: #f2f2f2;border-radius: 10px;padding: 10px 15px 0;margin-top: 15px;">
                                                        <div class="row">
                                                            <div class="col-md-4">
                                                                <div class="form-group">
                                                                    <label for=""><strong>Ghi chú</strong></label>
                                                                    <p style="margin-top: 7px;">
                                                                        <%= detail.hotel.note %>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-4" style="text-align: center;">
                                                                <div class="form-group">
                                                                    <label for="">
                                                                        <strong>Chính sách trẻ em</strong>
                                                                    </label>
                                                                    <p style="margin-top: 7px;">
                                                                        <%= detail.hotel.child_policy %>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-4" style="text-align: right;">
                                                                <div class="form-group">
                                                                    <label for="">
                                                                        <strong>Dịch vụ miễn phí</strong>
                                                                    </label>
                                                                    <p style="margin-top: 7px;">
                                                                        <%= detail.hotel.free_service %>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
